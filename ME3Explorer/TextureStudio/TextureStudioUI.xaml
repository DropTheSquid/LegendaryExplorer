<Window x:Class="ME3Explorer.TextureStudio.TextureStudioUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ME3Explorer.TextureStudio"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="Texture Studio" Height="450" Width="800">
    <xctk:BusyIndicator>
        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="Itch">
                    <MenuItem Header="Load vanilla texture map" Click="LTM"/>
                </MenuItem>
            </Menu>
            <DockPanel>

            </DockPanel>
        </DockPanel>
    </xctk:BusyIndicator>
</Window>
